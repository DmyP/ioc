<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="tweetRepository2"
          class="ua.rd.repository.InMemTweetRepository"
          init-method="init"
    />

    <bean name="tweetRepository1"
          autowire-candidate="false"
          class="ua.rd.repository.InMemTweetRepository"
          init-method="init">
        <qualifier></qualifier>
    </bean>

    <bean name="tweet" class="ua.rd.domain.Tweet">
        <constructor-arg type="java.lang.String" value="Tweet_repo"/>
        <constructor-arg type="ua.rd.domain.User"><null/></constructor-arg>
    </bean>
</beans>